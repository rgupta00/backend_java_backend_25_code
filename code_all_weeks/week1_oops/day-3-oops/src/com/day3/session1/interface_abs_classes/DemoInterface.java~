package com.day3.session1.interface_abs_classes;
class  A{
    int i=7;
    void show(){
        System.out.println("show of class A");
    }
}
class B{
    int i=70;
    void show(){
        System.out.println("show of class B");
    }
}

class D extends A, B{

    void show(){
        System.out.println("hello");
    }
}





